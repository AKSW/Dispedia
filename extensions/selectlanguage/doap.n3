@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owconfig: <http://ns.ontowiki.net/SysOnt/ExtensionConfig/> .
@prefix extension: <http://ns.ontowiki.net/Extensions/> .
@prefix event: <http://ns.ontowiki.net/SysOnt/Events/> .
@prefix : <http://ns.ontowiki.net/Extensions/selectlanguage> .
:selectlanguage a doap:Project;
  owconfig:enabled "1"^^xsd:boolean;
  doap:name "Select Language";
  doap:description "This plugin is used for switching to a specific language";
  doap:maintainer "Michael Martin";
  owconfig:helperEvents event:onPostBootstrap;
  owconfig:helperEvents event:onBeforeInitController;
  owconfig:config [
  a owconfig:Config;
  owconfig:id "languages";
  rdfs:comment "fixme";
  :en "english (en)";
  :de "deutsch (de)";
  :zh "汉语 (zh)".
] ; 
 owconfig:config [
  a owconfig:Config;
  owconfig:id "langzages";
  rdfs:comment "fixme";
  :ru "русский язык (ru)".
]