@prefix event: <http://ns.ontowiki.net/SysOnt/Events/> .
@prefix : <http://ns.ontowiki.net/Extensions/hideproperties/> .

<> foaf:primaryTopic :hideproperties.
:hideproperties a doap:Project;
  owconfig:privateNamespace <http://ns.ontowiki.net/Extensions/hideproperties/>;
  owconfig:enabled "false"^^xsd:boolean;
  doap:name "HideProperties";
  doap:description "A plug-in that hides defined Properties in the ressourceview";
  doap:maintainer "Niederstaetter Michael ,  Maier Christian";
  owconfig:helperEvents event:onPropertiesActionData;
  owconfig:config [
      a owconfig:Config;
      owconfig:id "hide";
      :property <http://ns.ontowiki.net/SysOnt/hidden>.
].
