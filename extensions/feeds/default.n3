@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owconfig: <http://ns.ontowiki.net/SysOnt/ExtensionConfig/> .
@prefix extension: <http://ns.ontowiki.net/Extensions/> .
@prefix event: <http://ns.ontowiki.net/SysOnt/Events/> .
@prefix : <http://ns.ontowiki.net/Extensions/feeds/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<> foaf:primaryTopic :feeds .

:feeds a doap:Project;
    doap:name "Feeds";
    doap:description "aggregate news from feeds which are related to a resource and displays them in a module";
    doap:maintainer <http://aksw.org/SebastianTramp>;
    owconfig:enabled "true"^^xsd:boolean;
    owconfig:hasModule :module;
    owconfig:privateNamespace <http://ns.ontowiki.net/Extensions/feeds/>;
    :relevant <http://lod2.eu/schema/feedRelevantFor>;
    :properties <http://rdfs.org/sioc/ns#feed>;
    :maxentries 3;
    :cachelivetime 86400.

:module a owconfig:Module;
    owconfig:priority 20;
#    owconfig:context <http://ns.ontowiki.net/Contexts/main/window/list>;
    owconfig:context <http://ns.ontowiki.net/Contexts/main/window/properties>.

:relevant rdfs:comment "additional feeds from resources linked to this resource".
:properties rdfs:comment "which feed properties should be checked".
:maxentries rdfs:comment "how many entries should be displayed".
:cachelivetime rdfs:comment "how long should we use the cached feeds".

