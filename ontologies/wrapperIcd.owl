<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
    <!ENTITY dbpedia "http://dbpedia.org/ontology/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY icd10 "http://www.owl-ontologies.com/icd10/#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY diswrap-icd10 "http://www.dispedia.de/wrapper/icd10/" >
    <!ENTITY dispediao "http://www.dispedia.de/o/" >
    <!ENTITY dispedia "http://www.dispedia.de/" >
]>

<rdf:RDF xmlns="&dispedia;"
     xml:base="&dispedia;"
     xmlns:rdfs="&rdfs;"
     xmlns:owl="&owl;"
     xmlns:xsd="&xsd;"
     xmlns:diswrap-icd10="&diswrap-icd10;"
     xmlns:rdf="&rdf;"
     xmlns:dispediao="&dispediao;"
     xmlns:icd10="&icd10;"
>

<owl:Ontology rdf:about="&dispedia;">
    <owl:imports rdf:about="&ic10;" />
</owl:Ontology>


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:ObjectProperty rdf:about="&diswrap-icd10;hasICDCode">
        <rdfs:label xml:lang="de">hat ICD-Code</rdfs:label>
        <rdfs:label xml:lang="en">has ICD code</rdfs:label>
        <rdfs:domain rdf:resource="&diswrap-icd10;ICDDisease"/>
        <rdfs:range rdf:resource="&icd10;ICD10_Chapter"/>
    </owl:ObjectProperty>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:Class rdf:about="&diswrap-icd10;ALS">
        <rdfs:label xml:lang="de">Amyotrophe Lateralsklerose</rdfs:label>
        <rdfs:label xml:lang="en">Amyotrophic lateral sclerosis</rdfs:label>
        <rdfs:subClassOf rdf:resource="&diswrap-icd10;ICDDisease"/>
      <rdfs:subClassOf rdf:resource="http://purl.bioontology.org/ontology/SNOMEDCT/86044005"/>
      <rdfs:subClassOf rdf:resource="http://purl.bioontology.org/ontology/MSH/D000690"/>
      <rdfs:subClassOf rdf:resource="http://purl.bioontology.org/ontology/MEDLINEPLUS/T135"/>
    </owl:Class>

    <owl:Class rdf:about="&diswrap-icd10;ICDDisease">
        <rdfs:label xml:lang="de">Krankheit mit ICD-Klassifikation</rdfs:label>
        <rdfs:label xml:lang="en">Disease with ICD classification</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dispediao;Disease"/>
    </owl:Class>
    
</rdf:RDF>

