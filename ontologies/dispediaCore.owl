]>

<rdf:RDF xmlns="&dispedia;"
     xml:base="&dispedia;"
     xmlns:rdfs="&rdfs;"
     xmlns:foaf="&foaf;"
     xmlns:owl="&owl;"
     xmlns:xsd="&xsd;"
     xmlns:dbpedia="&dbpedia;"
     xmlns:rdf="&rdf;"
     xmlns:ccns="&ccns;"
     xmlns:dct="&dct;"
     xmlns:dispediao="&dispediao;"
     xmlns:seo="&seo;"
     xmlns:gr="&gr;"
     xmlns:var="&var;"
     xmlns:swrl="&swrl;"
     xmlns:xml="&xml;"
     xmlns:icd="&icd;"
     xmlns:s="&s;"
     >
    <owl:Ontology rdf:about="&dispedia;">
        <rdfs:label xml:lang="de">Dispedia Vokabular (Kern Ontologie)</rdfs:label>
        <rdfs:label xml:lang="en">Dispedia Vocabulary (Core Ontology)</rdfs:label>
        <owl:versionInfo rdf:datatype="&rdf;PlainLiteral">0.7</owl:versionInfo>
        <rdfs:comment xml:lang="de">Diese Ontologie (Vokabular) enthält alle Kern-Konzepte des Dispedia Informationslogistik Systems.</rdfs:comment>
        <rdfs:comment xml:lang="en">This is the Core Ontology of the Dispedia Information Logistics System. All concepts are included needed to represent 
            information for and about the patient.</rdfs:comment>
        <dct:contributor>Romy Elze</dct:contributor>
        <dct:contributor>Michael Martin</dct:contributor>
        <dct:contributor>Tom-Michael Hesse</dct:contributor>
        <dct:contributor>Lars Eidam</dct:contributor>
        <ccns:license rdf:resource="http://creativecommons.org/licenses/by/3.0/" />
    </owl:Ontology>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&owl;versionInfo"/>
    <owl:AnnotationProperty rdf:about="&rdfs;label"/>
    <owl:AnnotationProperty rdf:about="&rdfs;comment"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    <!-- 
    ######################################################################################
    properties from Person
    -->
    <owl:ObjectProperty rdf:about="&dispediao;isDoctorOf">
        <rdfs:label xml:lang="de">ist Arzt von</rdfs:label>
        <rdfs:label xml:lang="en">is doctor of</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;Person"/>
        <rdfs:range rdf:resource="&dispediao;Patient"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&dispediao;isFamilyDoctorOf">
        <rdfs:label xml:lang="de">ist Hausarzt von</rdfs:label>
        <rdfs:label xml:lang="en">is family doctor of</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&dispediao;isDoctorOf"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&dispediao;isInARelationshipWith">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="de">Lebensgefährte / Ehepartner</rdfs:label>
        <rdfs:label xml:lang="en">Cohabitee / Marriage Partner</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;Person"/>
        <rdfs:range rdf:resource="&dispediao;Person"/>
        <rdfs:subPropertyOf rdf:resource="&s;relatedTo"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&dispediao;receivesProductOrService">
        <rdfs:label xml:lang="de">erhält Produkt oder Service</rdfs:label>
        <rdfs:label xml:lang="en">receives Product or Service</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;Person"/>
        <rdfs:range rdf:resource="&gr;ProductOrService"/>
        <rdfs:comment xml:lang="de">Die Eigenschaft 'erhältProduktOderService' bestimmt welche Produkte oder Services eine Person erhält.</rdfs:comment>
    </owl:ObjectProperty>
    
    <!-- 
    ######################################################################################
    properties from Patient
    -->
    <owl:ObjectProperty rdf:about="&dispediao;hasDisease">
        <rdfs:label xml:lang="de">hatKrankheit</rdfs:label>
        <rdfs:label xml:lang="en">hasDisease</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;Patient"/>
        <rdfs:range rdf:resource="&dispediao;Disease"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&dispediao;hasHealthState">
        <rdfs:label xml:lang="de">enthält Gesundheitsstatus</rdfs:label>
        <rdfs:label xml:lang="en">includes health state</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;Patient"/>
        <rdfs:range rdf:resource="&dispediao;HealthState"/>
    </owl:ObjectProperty>

    <!-- 
    ######################################################################################
    properties from Healthstate (includes is used for information about the patient)
    -->
     
    <owl:ObjectProperty rdf:about="&dispediao;accepts">
        <rdfs:label xml:lang="de">akzeptiert</rdfs:label>
        <rdfs:label xml:lang="en">accepts</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;HealthState"/>
        <rdfs:range rdf:resource="&dispediao;Proposal"/>
        <owl:propertyDisjointWith rdf:resource="&dispediao;denies"/>
        <rdfs:comment xml:lang="de">Die Objekteigenschaft 'akzeptiert' kennzeichnet einen zugewiesenen Vorschlag als akzeptierte Entscheidung.</rdfs:comment>
        <rdfs:comment xml:lang="en">The ObjectProperty 'accepts' indicates an alllocated proposal as accepted decision.</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&dispediao;denies">
        <rdfs:label xml:lang="de">lehnt ab</rdfs:label>
        <rdfs:label xml:lang="en">denies</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;HealthState"/>
        <rdfs:range rdf:resource="&dispediao;Proposal"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&dispediao;includes">
        <rdfs:label xml:lang="de">beeinhaltet</rdfs:label>
        <rdfs:label xml:lang="en">includes</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&dispediao;includesHealthProperties">
        <rdfs:label xml:lang="de">beinhaltet Gesundheits Eigenschaften</rdfs:label>
        <rdfs:label xml:lang="en">includes health properties</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;HealthState"/>
        <rdfs:range rdf:resource="&dispediao;PropertySet"/>
        <rdfs:subPropertyOf rdf:resource="&dispediao;includes"/>
    </owl:ObjectProperty>
   
    <owl:ObjectProperty rdf:about="&dispediao;includesSymptoms">
        <rdfs:label xml:lang="de">beeinhaltet Symptome</rdfs:label>
        <rdfs:label xml:lang="en">includes  symptoms</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;HealthState"/>
        <rdfs:range rdf:resource="&dispediao;SymptomSet"/>
        <rdfs:subPropertyOf rdf:resource="&dispediao;includes"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&dispediao;receivedProposalDescription">
        <rdfs:label xml:lang="de">Vorschlagsbeschreibungstext erhalten</rdfs:label>
        <rdfs:label xml:lang="en">received ProposalDescription</rdfs:label>
        <rdfs:range rdf:resource="&dispediao;HealthState"/>
        <rdfs:domain rdf:resource="&dispediao;ProposalDescription"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&dispediao;readProposalDescription">
        <rdfs:label xml:lang="de">Vorschlagstext gelesen</rdfs:label>
        <rdfs:label xml:lang="en">read ProposalDescription</rdfs:label>
        <rdfs:range rdf:resource="&dispediao;HealthState"/>
        <rdfs:domain rdf:resource="&dispediao;ProposalDescription"/>
    </owl:ObjectProperty>
    
    <!-- ######################################################################################
         properties from Proposal
     -->

    <owl:ObjectProperty rdf:about="&dispediao;appropriateFor">
        <rdfs:label xml:lang="de">passend für</rdfs:label>
        <rdfs:label xml:lang="en">appropriate for</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&dispediao;appropriateForProperties">
        <rdfs:label xml:lang="de">passend für Eigenschaften</rdfs:label>
        <rdfs:label xml:lang="en">appropriate for properties</rdfs:label>
        <rdfs:range rdf:resource="&dispediao;PropertySet"/>
        <rdfs:domain rdf:resource="&dispediao;Proposal"/>
        <rdfs:subPropertyOf rdf:resource="&dispediao;appropriateFor"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&dispediao;appropriateForSymptoms">
        <rdfs:label xml:lang="de">passend für Symptome</rdfs:label>
        <rdfs:label xml:lang="en">appropriate for symptoms</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;Proposal"/>
        <rdfs:range rdf:resource="&dispediao;SymptomSet"/>
        <rdfs:subPropertyOf rdf:resource="&dispediao;appropriateFor"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&dispediao;containsProposalComponent">
        <rdfs:label xml:lang="de">enthält Aktion</rdfs:label>
        <rdfs:label xml:lang="en">contains action</rdfs:label>
        <rdfs:range rdf:resource="&dispediao;ProposalComponent"/>
        <rdfs:domain rdf:resource="&dispediao;Proposal"/>
        <rdfs:subPropertyOf rdf:resource="&dispediao;contains"/>
    </owl:ObjectProperty>
    
    <!-- 
      ######################################################################################
      properties from ProposalComponent (contains is used for information for the patient)
    -->

    <owl:ObjectProperty rdf:about="&dispediao;contains">
        <rdfs:label xml:lang="de">enthält</rdfs:label>
        <rdfs:label xml:lang="en">contains</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&dispediao;containsDrug">
        <rdfs:label xml:lang="de">enthält Arzneiu</rdfs:label>
        <rdfs:label xml:lang="en">contains drugs</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;ProposalComponent"/>
        <rdfs:range rdf:resource="&dispediao;Drug"/>
        <rdfs:subPropertyOf rdf:resource="&dispediao;contains"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&dispediao;containsFormality">
        <rdfs:label xml:lang="de">enthält Formalität</rdfs:label>
        <rdfs:label xml:lang="en">contains formality</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;ProposalComponent"/>
        <rdfs:range rdf:resource="&dispediao;Formality"/>
        <rdfs:subPropertyOf rdf:resource="&dispediao;contains"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&dispediao;containsMedicalDevice">
        <rdfs:label xml:lang="de">enthält Medizinprodukt</rdfs:label>
        <rdfs:label xml:lang="en">contains medical device</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;ProposalComponent"/>
        <rdfs:range rdf:resource="&dispediao;MedicalDevice"/>
        <rdfs:subPropertyOf rdf:resource="&dispediao;contains"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&dispediao;containsProposalDescription">
        <rdfs:label xml:lang="de">enthält Informationstext</rdfs:label>
        <rdfs:label xml:lang="en">contains Proposal Description Text</rdfs:label>
        <rdfs:range rdf:resource="&dispediao;ProposalComponent"/>
        <rdfs:domain rdf:resource="&dispediao;ProposalDescription"/>
        <rdfs:subPropertyOf rdf:resource="&dispediao;contains"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&dispediao;containsAssistiveProduct">
        <rdfs:label xml:lang="de">enthält Assistenzprodukt</rdfs:label>
        <rdfs:label xml:lang="en">contains product</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;ProposalComponent"/>
        <rdfs:range rdf:resource="&dispediao;AssistiveProduct"/>
        <rdfs:subPropertyOf rdf:resource="&dispediao;contains"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&dispediao;containsService">
        <rdfs:label xml:lang="de">enthält Service</rdfs:label>
        <rdfs:label xml:lang="en">contains service</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;ProposalComponent"/>
        <rdfs:range rdf:resource="&seo;Service"/>
        <rdfs:subPropertyOf rdf:resource="&dispediao;contains"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&dispediao;successorOf">
        <rdfs:label xml:lang="de">Nachfolger von</rdfs:label>
        <rdfs:label xml:lang="en">successor of</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&dispediao;successorOfProposalComponent">
        <rdfs:label xml:lang="de">Nachfolger der Handlung</rdfs:label>
        <rdfs:label xml:lang="en">successor of action</rdfs:label>
        <rdfs:range rdf:resource="&dispediao;ProposalComponent"/>
        <rdfs:domain rdf:resource="&dispediao;ProposalComponent"/>
        <rdfs:subPropertyOf rdf:resource="&dispediao;successorOf"/>
    </owl:ObjectProperty>
    
    <!--
    ######################################################################################
    properties for ProposalDescription
    -->
    <owl:ObjectProperty rdf:about="&dispediao;usefulFor">
        <rdfs:label xml:lang="de">nützlich für</rdfs:label>
        <rdfs:label xml:lang="en">useful for</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;ProposalDescription"/>
        <rdfs:range rdf:resource="&seo;Organization"/>
    </owl:ObjectProperty>

    <!-- 
    ######################################################################################
    properties for rules for matching type of propal to type of patient
    -->
    <owl:ObjectProperty rdf:about="&dispediao;suitableFor">
        <rdfs:label xml:lang="de">geeignet für</rdfs:label>
        <rdfs:label xml:lang="en">suitable for</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&dispediao;suitableForSensible">
        <rdfs:label xml:lang="de">geeignet für sensible</rdfs:label>
        <rdfs:label xml:lang="en">suitable for sensible</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&dispediao;suitableFor"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&dispediao;suitableForExperienced">
        <rdfs:label xml:lang="de">geeignet für Erfahrene</rdfs:label>
        <rdfs:label xml:lang="en">suitable for experienced</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&dispediao;suitableFor"/>
    </owl:ObjectProperty>
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Rules to define the semantics of dispediao:suitableFor
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    <rdf:Description rdf:about="&var;x">
        <rdf:type rdf:resource="&swrl;Variable"/>
    </rdf:Description>

    <rdf:Description rdf:about="&var;y">
        <rdf:type rdf:resource="&swrl;Variable"/>
    </rdf:Description>

    <rdf:Description rdf:nodeID="genid1">
        <rdf:type rdf:resource="&swrl;ClassAtom"/>
        <swrl:argument1 rdf:resource="&var;y"/>
        <swrl:classPredicate rdf:resource="&dispediao;Patient"/>
    </rdf:Description>

    <rdf:Description rdf:nodeID="genid2">
        <rdf:type rdf:resource="&swrl;ClassAtom"/>
        <swrl:argument1  rdf:resource="&var;x"/>
        <swrl:classPredicate  rdf:resource="&dispediao;ProposalDescription"/>
    </rdf:Description>

    <rdf:Description rdf:nodeID="genid3">
        <rdf:type rdf:resource="&swrl;AtomList"/>
        <rdf:rest rdf:resource="&rdf;nil"/>
        <rdf:first rdf:nodeID="genid2"/>
    </rdf:Description>

    <rdf:Description rdf:nodeID="genid4">
        <rdf:type rdf:resource="&swrl;AtomList"/>
        <rdf:first rdf:nodeID="genid1"/>
        <rdf:rest rdf:nodeID="genid3"/>
    </rdf:Description>

    <rdf:Description rdf:nodeID="genid5">
        <rdf:type rdf:resource="&swrl;IndividualPropertyAtom"/>
        <swrl:propertyPredicate  rdf:resource="&dispediao;suitableFor"/>
        <swrl:argument1  rdf:resource="&var;x"/>
        <swrl:argument2  rdf:resource="&var;y"/>
    </rdf:Description>

    <rdf:Description rdf:nodeID="genid6">
        <rdf:type rdf:resource="&swrl;AtomList"/>
        <rdf:rest rdf:resource="&rdf;nil"/>
        <rdf:first rdf:nodeID="genid5"/>
    </rdf:Description>

    <rdf:Description rdf:nodeID="genid7">
        <rdf:type rdf:resource="&swrl;Imp"/>
        <swrl:body  rdf:nodeID="genid4"/>
        <swrl:head  rdf:nodeID="genid6"/>
    </rdf:Description>

    <rdf:Description rdf:nodeID="genid1a">
        <rdf:type rdf:resource="&swrl;ClassAtom"/>
        <swrl:argument1 rdf:resource="&var;y"/>
        <swrl:classPredicate rdf:resource="&dispediao;ExperiencedPatient"/>
    </rdf:Description>

    <rdf:Description rdf:nodeID="genid2a">
        <rdf:type rdf:resource="&swrl;ClassAtom"/>
        <swrl:classPredicate  rdf:resource="&dispediao;ExperiencedProposalDescription"/>
        <swrl:argument1  rdf:resource="&var;x"/>
    </rdf:Description>

    <rdf:Description rdf:nodeID="genid3a">
        <rdf:type rdf:resource="&swrl;AtomList"/>
        <rdf:rest rdf:resource="&rdf;nil"/>
        <rdf:first rdf:nodeID="genid2a"/>
    </rdf:Description>

    <rdf:Description rdf:nodeID="genid4a">
        <rdf:type rdf:resource="&swrl;AtomList"/>
        <rdf:first rdf:nodeID="genid1a"/>
        <rdf:rest rdf:nodeID="genid3a"/>
    </rdf:Description>

    <rdf:Description rdf:nodeID="genid5a">
        <rdf:type rdf:resource="&swrl;IndividualPropertyAtom"/>
        <swrl:propertyPredicate  rdf:resource="&dispediao;suitableForExperienced"/>
        <swrl:argument1  rdf:resource="&var;x"/>
        <swrl:argument2  rdf:resource="&var;y"/>
    </rdf:Description>

    <rdf:Description rdf:nodeID="genid6a">
        <rdf:type rdf:resource="&swrl;AtomList"/>
        <rdf:rest rdf:resource="&rdf;nil"/>
        <rdf:first rdf:nodeID="genid5a"/>
    </rdf:Description>

    <rdf:Description rdf:nodeID="genid7a">
        <rdf:type rdf:resource="&swrl;Imp"/>
        <swrl:body  rdf:nodeID="genid4a"/>
        <swrl:head  rdf:nodeID="genid6a"/>
    </rdf:Description>

    <rdf:Description rdf:nodeID="genid1b">
        <rdf:type rdf:resource="&swrl;ClassAtom"/>
        <swrl:argument1 rdf:resource="&var;y"/>
        <swrl:classPredicate rdf:resource="&dispediao;SensiblePatient"/>
    </rdf:Description>

    <rdf:Description rdf:nodeID="genid2b">
        <rdf:type rdf:resource="&swrl;ClassAtom"/>
        <swrl:classPredicate  rdf:resource="&dispediao;SensibleProposalDescription"/>
        <swrl:argument1  rdf:resource="&var;x"/>
    </rdf:Description>

    <rdf:Description rdf:nodeID="genid3b">
        <rdf:type rdf:resource="&swrl;AtomList"/>
        <rdf:rest rdf:resource="&rdf;nil"/>
        <rdf:first rdf:nodeID="genid2b"/>
    </rdf:Description>

    <rdf:Description rdf:nodeID="genid4b">
        <rdf:type rdf:resource="&swrl;AtomList"/>
        <rdf:first rdf:nodeID="genid1b"/>
        <rdf:rest rdf:nodeID="genid3b"/>
    </rdf:Description>

    <rdf:Description rdf:nodeID="genid5b">
        <rdf:type rdf:resource="&swrl;IndividualPropertyAtom"/>
        <swrl:propertyPredicate  rdf:resource="&dispediao;suitableForSensible"/>
        <swrl:argument1  rdf:resource="&var;x"/>
        <swrl:argument2  rdf:resource="&var;y"/>
    </rdf:Description>

    <rdf:Description rdf:nodeID="genid6b">
        <rdf:type rdf:resource="&swrl;AtomList"/>
        <rdf:rest rdf:resource="&rdf;nil"/>
        <rdf:first rdf:nodeID="genid5b"/>
    </rdf:Description>

    <rdf:Description rdf:nodeID="genid7b">
        <rdf:type rdf:resource="&swrl;Imp"/>
        <swrl:body  rdf:nodeID="genid4b"/>
        <swrl:head  rdf:nodeID="genid6b"/>
    </rdf:Description>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!--
    ######################################################################################
    properties of Disease
    -->
    <owl:DatatypeProperty rdf:about="&dispediao;startDate">
        <rdfs:label xml:lang="de">Startdatum</rdfs:label>
        <rdfs:label xml:lang="en">start date</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;Disease"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&dispediao;endDate">
        <rdfs:label xml:lang="de">Enddatum</rdfs:label>
        <rdfs:label xml:lang="en">end date</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;Disease"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    
    <!-- 
    ######################################################################################
    properties of ProposalDescription
    -->
    <owl:DatatypeProperty rdf:about="&dispediao;content">
        <rdfs:label xml:lang="de">Inhalt</rdfs:label>
        <rdfs:label xml:lang="en">content</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;ProposalDescription"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>

    <!-- 
    ######################################################################################
    properties of Person and Patient(...which could not be reused from foaf, schema or dbpedia... until a patient data ontology is available
    -->
    <owl:DatatypeProperty rdf:about="&dispediao;journeyToWork">
        <rdfs:label xml:lang="de">Arbeitsweg</rdfs:label>
        <rdfs:label xml:lang="en">Journey to work</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;Person"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&dispediao;birthName">
        <rdfs:label xml:lang="de">Geburtsname</rdfs:label>
        <rdfs:label xml:lang="en">Name at birth</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;Person"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&dispediao;familyState">
        <rdfs:label xml:lang="de">Familiäre Verhältnisse</rdfs:label>
        <rdfs:label xml:lang="en">family state</rdfs:label>
        <rdfs:domain rdf:resource="&dispediao;Person"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
